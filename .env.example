# Robustty Music Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# Optional Platform Configuration
ODYSEE_ENABLED=false
RUMBLE_ENABLED=false
RUMBLE_API_TOKEN=your_rumble_api_token_here
# For Rumble Apify support (optional)
USE_RUMBLE_EXTRACTOR=false
APIFY_API_TOKEN=your_apify_api_token_here

# Performance Configuration
SEARCH_TIMEOUT=30
STREAM_TIMEOUT=300
MAX_QUEUE_SIZE=100

# Redis Configuration (optional - uses defaults if not set)
# Redis caching helps reduce API calls and improve performance
REDIS_ENABLED=false
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Browser Profile Path (optional - for cookie extraction)
# CHROME_PROFILE_PATH=/path/to/chrome/profile

# Cookie refresh interval (in seconds)
COOKIE_REFRESH_INTERVAL=3600

# Logging level
LOG_LEVEL=INFO

# Metrics server configuration
METRICS_PORT=8080