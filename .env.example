# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_PREFIX=!

# YouTube Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here

# PeerTube Configuration
PEERTUBE_INSTANCES=https://framatube.org,https://video.ploud.fr

# Odysee Configuration
ODYSEE_ENABLED=true

# Rumble Configuration
RUMBLE_ENABLED=true

# Cookie Extraction
COOKIE_REFRESH_INTERVAL=3600
BROWSER_PROFILE_PATH=/home/user/.config/google-chrome

# Logging
LOG_LEVEL=INFO
LOG_FILE_SIZE=10485760
LOG_BACKUP_COUNT=5

# Performance
MAX_QUEUE_SIZE=100
SEARCH_TIMEOUT=10
STREAM_TIMEOUT=30

# Docker Configuration
COMPOSE_PROJECT_NAME=robustty