# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_discord_client_id

# YouTube API
YOUTUBE_API_KEY=your_youtube_api_key

# Voice Commands (disabled by default, auto-enabled if TTS is enabled)
ENABLE_VOICE_COMMANDS=true  # Set to true to enable voice commands
OPENAI_API_KEY=your_openai_api_key  # Required if voice commands are enabled

# Text-to-Speech (Optional - will auto-enable voice commands)
TTS_ENABLED=false  # Set to true for voice responses
ELEVENLABS_API_KEY=your_elevenlabs_api_key  # Required if TTS is enabled
ELEVENLABS_VOICE_ID=ErXwobaYiN019PkySvjV  # Optional, defaults provided
ELEVENLABS_MODEL_ID=eleven_flash_v2_5  # Optional, uses fastest model

# Redis Configuration
REDIS_URL=redis://redis:6379

# Neko Configuration
NEKO_INTERNAL_URL=http://neko:8080
NEKO_PASSWORD=your_neko_password
NEKO_NAT1TO1=  # Your public IP if behind NAT

# Admin Configuration (optional)
ADMIN_ROLE_ID=your_admin_role_id
ADMIN_NOTIFICATION_WEBHOOK=your_discord_webhook_url

# Environment Settings
NODE_ENV=production
LOG_LEVEL=info
VOICE_IDLE_TIMEOUT_MS=300000  # 5 minutes